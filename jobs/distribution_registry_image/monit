check process distribution_registry_image
  with pidfile /var/vcap/sys/run/distribution_registry_image/distribution_registry_image.pid
  start program "/var/vcap/jobs/distribution_registry_image/bin/monit_debugger install_ctl '/var/vcap/jobs/distribution_registry_image/bin/install_ctl start'"
  stop program "/var/vcap/jobs/distribution_registry_image/bin/monit_debugger install_ctl '/var/vcap/jobs/distribution_registry_image/bin/install_ctl stop'"
  group vcap
